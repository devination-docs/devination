{
  "name": "devination",
  "displayName": "devination",
  "description": "code plugin for querying docs through devination",
  "version": "0.0.1",
  "publisher": "devination",
  "engines": {
    "vscode": "^1.10.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.devination"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "languages": [
      {
        "id": "haskell",
        "aliases": [
          "Haskell",
          "haskell"
        ],
        "extensions": [
          ".hs",
          ".lhs"
        ]
      }
    ],
    "commands": [
      {
        "command": "extension.devination",
        "title": "Search in devination"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "cross-spawn": "^5.1.0",
    "open": "0.0.5",
    "opn": "^4.0.2",
    "spawn-sync": "^1.0.15"
  }
}
